package greeter

object Hello extends App{
  println("Hello, World!")

}


object sandbozz {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  val x = 1                                       //> x  : Int = 1
  def increase(i: Int) = i + 1                    //> increase: (i: Int)Int
  increase(x)                                   //> res0: Int = 2
  println("Now the value of x is:" + x)
}

